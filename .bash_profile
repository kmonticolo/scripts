#   for i in `cat 2polhosty |sed -e 's/^pol//g' -e 's/\..*$//'`;do echo  Host $i ;echo  HostName  pol${i}.polcard.com.pl;done >> ~/.ssh/config 
# bash completion autouzupelnianie na podstawie z .ssh/config
function _ssh_completion() {

perl -ne 'print "$1 " if /^Host (.+)$/' ~/.ssh/config

}

complete -W "$(_ssh_completion)" ssh

alias s='source ~/.bash_profile'
# dodaje alias ssh do .bash_profile (wymagane jest uzycie funkcji, zeby obsluzyc argumenty typu $1
function dodajssh() {
  if [ ! -z $1 ];then {
  echo "alias $1='ssh $1'" >>~/.bash_profile
  krotki=`echo $1|sed 's/^pol//'`
  echo "alias $krotki='ssh $1'" >>~/.bash_profile
  source ~/.bash_profile
  }
fi
}
alias dodajssh=dodajssh

### deploy vpos na testach z uzyciem najnowszej paczki z ~/Downloads
  export ear=`ls -1ta ~/Downloads/vpos*zip|head -1` # najnowszy zip vposa z traca
function deployvpos() {
  ear=`ls -1ta ~/Downloads/vpos*zip|head -1` # najnowszy zip vposa z traca
  #export md5ear=`md5 $ear|cut -d' ' -f4`
  if [ ! -z $ear ]; then {
    nocomment /home/kmonticolo/ansible/playbooks/vpostest/hosts ; echo
    #echo $ear 
    #echo ansible-playbook -i /home/kmonticolo/ansible/playbooks/vpostest/hosts /home/kmonticolo/ansible/playbooks/vpostest/site.yml -K -e "ear=${ear} md5ear=${md5ear}" $1
    echo ansible-playbook -i /home/kmonticolo/ansible/playbooks/vpostest/hosts /home/kmonticolo/ansible/playbooks/vpostest/site.yml -K -e "ear=${ear} " $@
    ansible-playbook -i /home/kmonticolo/ansible/playbooks/vpostest/hosts /home/kmonticolo/ansible/playbooks/vpostest/site.yml -K -e "ear=${ear}" $@
  }
  fi
}
